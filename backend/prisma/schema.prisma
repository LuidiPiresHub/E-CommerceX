generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Product {
  id        String   @id
  title     String
  price     Float
  thumbnail String
  quantity  Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
